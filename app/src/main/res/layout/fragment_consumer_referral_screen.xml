<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".feature.consumer.ui.consumptionFragments.ConsumerReferralScreenFragment">


    <ImageView
        android:id="@+id/consumer_referral_screen_imageButton1"
        android:layout_width="37dp"
        android:layout_height="29dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="52dp"
        android:background="@color/white"
        android:contentDescription="@null"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/consumer_referral_screen_textView2"
        android:layout_width="54dp"
        android:layout_height="24dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/manrope_light"
        android:text="@string/back"
        app:layout_constraintStart_toEndOf="@+id/consumer_referral_screen_imageButton1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/consumer_referral_screen_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/manrope_bold"
        android:text="@string/invite_friends"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.364"
        app:layout_constraintStart_toEndOf="@+id/consumer_referral_screen_textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/consumer_referral_screen_imageView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="24dp"
        android:contentDescription="@null"
        android:src="@drawable/logos"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_textView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_imageButton1"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_textView3" />

    <TextView
        android:id="@+id/consumer_referral_screen_textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/manrope_bold"
        android:text="@string/refer_your_friends_earn_ncash_back"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_imageView4"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_imageView4"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_imageView4" />

    <TextView
        android:id="@+id/consumer_referral_screen_textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/manrope_light"
        android:text="@string/invite_your_friends_to_join_aqua_and_get_500_ncash_back_for_each_friend_that_joins_using_nyour_referral_code"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_textView5"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_textView5"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_textView5" />

    <ImageView
        android:id="@+id/consumer_referral_screen_imageView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="16dp"
        android:src="@drawable/rectangle"
        android:contentDescription="@null"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_textView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_textView6"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_textView6" />

    <TextView
        android:id="@+id/consumer_referral_screen_textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/manrope_light"
        android:text="@string/my_referrals"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_imageView7"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_imageView7"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_imageView7" />

    <ImageView
        android:id="@+id/consumer_referral_screen_imageView9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@null"
        android:src="@drawable/referrals"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_textView8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_textView8"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_textView8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/consumer_referral_screen_button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="48dp"
        android:drawableStart="@drawable/ic_upload"
        android:gravity="center"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="@string/invite_more_friends"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/consumer_referral_screen_imageView9"
        app:layout_constraintStart_toStartOf="@+id/consumer_referral_screen_imageView9"
        app:layout_constraintTop_toBottomOf="@+id/consumer_referral_screen_imageView9"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>

