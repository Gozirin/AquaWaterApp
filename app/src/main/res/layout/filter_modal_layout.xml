<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/filter_modal_LO"
    android:background="@drawable/consumer_details_modal_bg">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_comsumer_profile_modal_line"
        tools:ignore="ContentDescription" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/filter_detail_modal_profile_detail_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/consumer_detail_modal_profile_detail"
            android:layout_marginLeft="130dp"
            android:layout_toEndOf="@id/consumer_detail_modal_profile_detail"
            android:src="@drawable/ic_close_24"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:id="@+id/consumer_detail_modal_profile_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/manrope_bold"
            android:text="@string/filter"
            android:textAlignment="center"
            android:textColor="@color/all_text_color"
            android:textFontWeight="700"
            android:textSize="18sp"
            tools:ignore="UnusedAttribute" />

    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/consumer_detail_modal_profile_detail"
        android:layout_marginTop="15dp"
        android:src="@drawable/ic_comsumer_profile_modal_line2"
        tools:ignore="ContentDescription,ObsoleteLayoutParam" />

    <RelativeLayout
        android:id="@+id/order_details_bottom_sheet_requester_name_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/filter_modal_name_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="Name"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/order_details_bottom_sheet_quantity_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/order_details_quantity_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="@string/date"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/order_details_bottom_sheet_phone_number_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/order_details_phone_number_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="@string/time"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="RtlHardcoded,UnusedAttribute" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/order_details_bottom_sheet_address_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/order_details_address_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="@string/items"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="RtlHardcoded,UnusedAttribute" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/order_details_bottom_sheet_description_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/order_details_description_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="@string/price"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="RtlHardcoded,UnusedAttribute" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/filter_modal_location_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/filter_modal_location_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/manrope_medium"
            android:padding="5dp"
            android:text="@string/location"
            android:textColor="@color/all_text_color"
            android:textFontWeight="400"
            android:textSize="16sp"
            tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
    </RelativeLayout>

</LinearLayout>