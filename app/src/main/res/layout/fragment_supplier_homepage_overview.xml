<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/fragment_supplier_homepage_overview_appbar" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="@color/app_primary_color_light">

                <SearchView
                    android:id="@+id/totalSupplyMade_supplier_searchview"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginHorizontal="15dp"
                    android:background="#ffffff"
                    android:iconifiedByDefault="false"
                    android:queryHint="Search for Consumers"
                    android:searchIcon="@drawable/ic_search" />

            </RelativeLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:drawableLeft="@drawable/ic_blocks"
                android:fontFamily="@font/manrope_medium"
                android:text=" Overview"
                android:textColor="#21334F"
                android:textFontWeight="400"
                android:textSize="24sp" />

            <!-- itemview 1 -->
            <androidx.cardview.widget.CardView
                android:id="@+id/supplier_homepage_overview_cardview1"
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginHorizontal="15dp"
                android:layout_marginTop="10dp"
                android:backgroundTint="#22147DF5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                card_view:cardElevation="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/supplier_homepage_overview_count_tv"
                        android:layout_width="103dp"
                        android:layout_height="65dp"
                        android:fontFamily="@font/manrope_bold"
                        android:paddingEnd="10dp"
                        android:text="98"
                        android:textAlignment="textEnd"
                        android:textColor="#21334F"
                        android:textFontWeight="700"
                        android:textSize="50sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.054"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.571" />

                    <TextView
                        android:id="@+id/supplier_homepage_overview_total_supplies_made_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/manrope_medium"
                        android:text="Total \nSupplies Made"
                        android:textColor="#21334F"
                        android:textFontWeight="400"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.056"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_count_tv"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.507" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_navigate_next_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.904"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_total_supplies_made_tv"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <!--itemview2-->
            <androidx.cardview.widget.CardView
                android:id="@+id/supplier_homepage_overview_cardview2"
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginHorizontal="15dp"
                android:layout_marginTop="10dp"
                android:backgroundTint="#22FFBE0B"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                card_view:cardElevation="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/supplier_homepage_overview_count2_tv"
                        android:layout_width="103dp"
                        android:layout_height="65dp"
                        android:fontFamily="@font/manrope_bold"
                        android:paddingEnd="10dp"
                        android:text="111"
                        android:textAlignment="textEnd"
                        android:textColor="#21334F"
                        android:textFontWeight="700"
                        android:textSize="50sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.054"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.571" />

                    <TextView
                        android:id="@+id/supplier_homepage_overview_total_supplies_made2_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/manrope_medium"
                        android:text="Total \norders received"
                        android:textColor="#21334F"
                        android:textFontWeight="400"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.056"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_count2_tv"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.507" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_navigate_next_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.904"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_total_supplies_made2_tv"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <!--itemview 3-->

            <androidx.cardview.widget.CardView
                android:id="@+id/supplier_homepage_overview_cardview3"
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginHorizontal="15dp"
                android:layout_marginTop="10dp"
                android:backgroundTint="#22EF233C"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                card_view:cardElevation="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/supplier_homepage_overview_count3_tv"
                        android:layout_width="103dp"
                        android:layout_height="65dp"
                        android:fontFamily="@font/manrope_bold"
                        android:paddingEnd="10dp"
                        android:text="32"
                        android:textAlignment="textEnd"
                        android:textColor="#21334F"
                        android:textFontWeight="700"
                        android:textSize="50sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.054"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.571" />

                    <TextView
                        android:id="@+id/supplier_homepage_overview_total_supplies_made3_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/manrope_medium"
                        android:text="Past \nPast Supplies"
                        android:textColor="#21334F"
                        android:textFontWeight="400"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.056"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_count3_tv"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.507" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_navigate_next_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.904"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_total_supplies_made3_tv"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <!--itemview 4-->
            <androidx.cardview.widget.CardView
                android:id="@+id/supplier_homepage_overview_cardview4"
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginHorizontal="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="50dp"
                android:backgroundTint="#2238B000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                card_view:cardElevation="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/supplier_homepage_overview_count4_tv"
                        android:layout_width="103dp"
                        android:layout_height="65dp"
                        android:fontFamily="@font/manrope_bold"
                        android:paddingEnd="10dp"
                        android:text="12"
                        android:textAlignment="textEnd"
                        android:textColor="#21334F"
                        android:textFontWeight="700"
                        android:textSize="50sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.054"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.571" />

                    <TextView
                        android:id="@+id/supplier_homepage_overview_total_supplies_made4_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/manrope_medium"
                        android:text="List of \nfavourite customers"
                        android:textColor="#21334F"
                        android:textFontWeight="400"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.056"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_count4_tv"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.507" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_navigate_next_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.904"
                        app:layout_constraintStart_toEndOf="@id/supplier_homepage_overview_total_supplies_made4_tv"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>